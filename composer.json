  "require-dev": {
    "couscous/couscous": "1.4.*",
    "phing/phing": "2.*",
    "phpunit/phpunit": "5.*",
    "phploc/phploc": "3.*",
    "sebastian/phpcpd": "2.*",
    "pdepend/pdepend": "2.*",
    "apigen/apigen": "4.*",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "BuildR\\TestTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BuildR\\TestTools\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
